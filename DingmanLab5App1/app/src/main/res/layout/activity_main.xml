<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="bryan.dingmanlab5app1.MainActivity">

    <ImageButton
        android:id="@+id/blount"
        android:layout_width="200dp"
        android:layout_height="162dp"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_gravity="left"
        app:srcCompat="@drawable/blount"
        android:tag="blount"/>

    <ImageButton
        android:id="@+id/division"
        android:layout_width="200dp"
        android:layout_height="162dp"
        android:layout_column="1"
        android:layout_row="0"
        android:layout_gravity="right"
        app:srcCompat="@drawable/division"
        android:tag="division"/>

    <ImageButton
        android:id="@+id/magnolia"
        android:layout_width="200dp"
        android:layout_height="162dp"
        android:layout_row="1"
        android:layout_column="0"
        android:layout_gravity="left"
        app:srcCompat="@drawable/magnolia"
        android:tag="magnolia"/>

    <ImageButton
        android:layout_width="200dp"
        android:layout_height="162dp"
        android:layout_row="1"
        android:layout_column="1"
        android:id="@+id/hardin"
        android:layout_gravity="right"
        app:srcCompat="@drawable/hardin"
        android:tag="hardin"/>

    <ImageButton
        android:layout_width="200dp"
        android:layout_height="162dp"
        android:layout_row="2"
        android:layout_column="0"
        android:id="@+id/strawberry"
        app:srcCompat="@drawable/strawberry"
        android:tag="strawberry"/>

</GridLayout>
